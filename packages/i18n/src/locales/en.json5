    memory_valid_until_placeholder: 'YYYY-MM-DD HH:MM',
    memory_temporary_badge: 'Temporary memory, valid until {{date}}',
    new_session_prompt_name_lock: 'The user is already known as {{name}}. Do NOT call the name/nickname tools again unless the user asks to change it.',
    new_session_prompt_projects_list: 'There are {{count}} projects tracked right now: {{list}}. Use project_list if you need details before working with them.',
    new_session_prompt_new_user: 'Greet the user, welcome them as your new user and offer your help or ask something more about them that you can save and remember for later, e.g., what is their name?',
    new_session_prompt_end: 'Now greet {{name}} and ask how you can help.',
